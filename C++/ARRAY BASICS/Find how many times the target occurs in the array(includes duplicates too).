#include <iostream>
#include <vector>
using namespace std;

int firstoccurence(vector<int> & arr, int target){
    int low= 0, high = arr.size() - 1, ans = -1;
    while (low<=high){
        int mid= low + (high- low)/2;
        if(arr[mid]== target){
            ans=mid;
            high = mid - 1;}
            
           else if(arr[mid] < target){
               low = mid + 1;
           }
           else {
               high = mid - 1;
           }
           
        }  return ans;
    }
    int lastoccurence(vector<int> & arr, int target){
    int low= 0, high = arr.size() - 1, ans = -1;
    while (low<=high){
        int mid= low + (high- low)/2;
        if(arr[mid]== target){
            ans=mid;
            low = mid + 1;}
            
           else if(arr[mid] > target){
               high = mid - 1;
           }
           else {
               low = mid + 1;
           }
}  return ans;
    }
    
    int main(){
        vector<int> arr= {1 , 2 , 2, 2, 4, 4, 4, 4,6};
        int target = 5;
        if(firstoccurence(arr, target) == -1){
            cout << " element is not in the array" ;
        }
        else{
        cout << lastoccurence(arr, target) -firstoccurence(arr, target) +1 << endl;
        }
        return 0;
    }
