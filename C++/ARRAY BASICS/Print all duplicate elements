#include <iostream>
using namespace std;

int main() {
    int n ;
    cout << " enter no of elements u want in array" << endl ;
    cin >> n;
    
    int arr[n];
    cout << " enter the elements of array" << endl; ;
    for( int i = 0; i < n ; i++) {
    cin >> arr[i] ;
      }
    
    for( int i = 0; i < n ; i++) {
        if( arr[i] == -1) continue;
        
        bool isduplicate = false;
        for( int j = i + 1; j < n ; j++){
            if( arr[i] == arr[j]){
                isduplicate = true;
                arr[j]= -1;
            }
        }
          if(isduplicate) {
              cout << "duplicate elements is " << arr[i] << endl;
              arr[i] = -1;
          }  
        }
    }
