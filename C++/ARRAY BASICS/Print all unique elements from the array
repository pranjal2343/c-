#include <iostream>
using namespace std;
int main(){
    int n ;
    cout<< "enter no of elements u want in array" << endl;
    cin >> n ;
    
    int arr[n];
    cout<< "enter elements of ur array"<< endl;
    for( int i = 0; i < n ; i++){
    cin >> arr[i];  }
    
    for( int i = 0; i < n ; i++){
        bool isunique = true;
        
        for (int j = 0; j < n ; j++){
            if( i!=j && arr[i] == arr[j]){
                isunique = false;
                break;
            } 
        }
    if (isunique) {
        cout << "unique elements are " <<arr[i] << " " ;} 
}
}
