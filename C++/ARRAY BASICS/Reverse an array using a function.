#include <iostream>
using namespace std;

void swapfunction(int arr[], int n ){
    int start = 0, end = n - 1;
    while( start < end) {
    swap(arr[start], arr[end]);
    start++ ;
    end--;
}
}
    int main() {
        int n ; 
        cout << " enter no of elements in array " << endl;
        cin >> n ;
        
        
        int arr[n];
        cout << " enter elemnents of array " << endl;
        for( int i = 0 ; i < n ; i++){
        cin >> arr[i];
        }
        swapfunction(arr, n );
        cout << " reversed array is " << endl;
        
        
        for( int i = 0; i < n ; i++){
            cout << arr[i] << " " ;
            
        }   cout << endl;
          
            return 0;
    }
