#include <iostream>
#include <unordered_map>

using namespace std;
int main(){
    int n ;
    cout<< "enter no of elements u want in array" << endl;
    cin >> n ;
    
    int arr[n];
    cout<< "enter elements of ur array"<< endl;
    for( int i = 0; i < n ; i++){
    cin >> arr[i];  
        
    }
    unordered_map<int,int>freq;
    
    for(int i = 0; i < n ; i++){
           freq[arr[i]]++;
    }
    
    for (auto pair : freq) {
        cout << pair.first << " occurs " << pair.second << "times" << endl;
        }
    
}
