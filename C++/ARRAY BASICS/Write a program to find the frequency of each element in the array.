#include <iostream>

using namespace std;
int main(){
    int n ;
    cout<< "enter no of elements u want in array" << endl;
    cin >> n ;
    
    int arr[n];
    cout<< "enter elements of ur array"<< endl;
    for( int i = 0; i < n ; i++){
    cin >> arr[i];  }
    
    bool visited[n]= {false};
    
    for(int i = 0; i < n ; i++){
        if (visited[i]== true)
        continue;
        
        int count = 1;
        for( int j = 1+ 1; j < n; j++){
            if(arr[i]== arr[j]) {
                count++;
                visited[j]= true;
                
            }
        }
        cout<< arr[i] << " occurs " << count << " times " << endl;
        
    }
}
    
