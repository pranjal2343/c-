#include <iostream>
using namespace std;

int main(){
    int n;
    cout << " enter no of elements of array  " << endl;
    cin >> n;
    
    int arr[n];
    cout << " enter elements of array " << endl;
   
    for( int i = 0; i < n ; i ++){
         cin >> arr[i];
    }
    
    
    
    for(int i = 0; i < n ; i++){
        if(arr[i] == -1) continue;
        
        int count = 1;
        for(int j = i + 1; j < n ; j++){
            if(arr[i] == arr[j]){
              
                count = count + 1;
                 arr[j] = -1;
                
            } 
            
        }  cout << arr[i] << "appeared " << count << "times "<< endl;
    } 
    
}
