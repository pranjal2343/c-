#include <iostream>
using namespace std;

void twosum( int arr[], int n , int target){
    for(int i = 0; i < n ; i ++){
        for( int j = i+ 1; j < n ; j++){
            if(arr[i] + arr[j] == target){
                cout << arr[i] << " " << arr[j] << endl;
                arr[i] = -1 ; arr[j] = -1;
                
            }
        }
    }
}
    int main(){
        
    int n ;
    cout << "enter no of elements u want in array "<< endl;
    cin >> n ;
    
    int arr[n];
    cout << "enter elements of array " << endl;
    for(int i = 0; i < n ;i++){
        cin >> arr[i] ;
        
    }
    int target;
    cout << "enter the target" << endl;
    cin >> target;
    
    twosum(arr, n , target);
    return 0;
    }
